{
  "name": "tmdbts",
  "version": "0.0.1",
  "type": "module",
  "description": "A type-safe, intuitive TypeScript library for the TMDB API",
  "main": "./dist/tmdbts.cjs",
  "module": "./dist/tmdbts.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/tmdbts.mjs",
      "require": "./dist/tmdbts.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "tmdb",
    "api",
    "typescript",
    "movies",
    "tv"
  ],
  "author": "Cesar Daniel <cesardaniel.9611@gmail.com",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@types/node": "^22.15.29",
    "@vitest/coverage-v8": "^3.2.2",
    "axios-mock-adapter": "^2.1.0",
    "happy-dom": "^17.6.3",
    "rolldown-plugin-dts": "^0.13.8",
    "typescript": "~5.8.3",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.2.1"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "dependencies": {
    "axios": "^1.9.0"
  }
}
